// check cargo for box subtype
switch(FEAT_ROADVEHS, SELF, check_box_cargo_class_spriteset_PZInz703_graphic_switch, hasbit(cargo_classes, CC_BULK) + 2 * hasbit(cargo_classes, CC_COVERED) + 4 * hasbit(cargo_classes, CC_LIQUID) + 8 * hasbit(cargo_classes, CC_REFRIGERATED) + 16 * hasbit(cargo_classes, CC_PIECE_GOODS) + 16 * hasbit(cargo_classes, CC_ARMOURED)) {  
1 : return bulk_uncovered_spritegroup_PZInz703_graphic;
2 : return good_spriteset_PZInz703_graphic;
3 : return bulk_covered_spritegroup_PZInz703_graphic;
4 : return oilbox_spritegroup_PZInz703_graphic;
8 : return good_spriteset_PZInz703_graphic;
16 : return good_spritegroup_PZInz703_graphic;
32 : return good_spriteset_PZInz703_graphic;
return good_spritegroup_PZInz703_graphic;
}
switch(FEAT_ROADVEHS, SELF, box_spriteset_PZInz703_graphic, cargo_type_in_veh) {  
BDMT : return bdmtbox_spritegroup_PZInz703_graphic;
BRCK : return bdmtbox_spritegroup_PZInz703_graphic;
STEL : return stelbox_spritegroup_PZInz703_graphic;
IRON : return stelbox_spritegroup_PZInz703_graphic;
METL : return stelbox_spritegroup_PZInz703_graphic;
WOOD : return woodbox_spritegroup_PZInz703_graphic;
WDPR : return wdprbox_spritegroup_PZInz703_graphic;
GRAI : return bulk_covered_spritegroup_PZInz703_graphic;
WHEA : return bulk_covered_spritegroup_PZInz703_graphic;
MAIL : return good_spriteset_PZInz703_graphic;
return check_box_cargo_class_spriteset_PZInz703_graphic_switch;
}
// check cargo for flat subtype
switch(FEAT_ROADVEHS, SELF, flat_spriteset_PZInz703_graphic, cargo_type_in_veh) {  

WOOD : return wood_spritegroup_PZInz703_graphic;
WDPR : return wdpr_spritegroup_PZInz703_graphic;
VEHI : return vehi_spritegroup_PZInz703_graphic;
ENSP : return vehi_spritegroup_PZInz703_graphic;

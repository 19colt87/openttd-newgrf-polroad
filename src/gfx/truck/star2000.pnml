// check cargo for box subtype
switch(FEAT_ROADVEHS, SELF, check_box_cargo_class_spriteset_Star2000_graphic_switch, hasbit(cargo_classes, CC_BULK) + 2 * hasbit(cargo_classes, CC_COVERED) + 4 * hasbit(cargo_classes, CC_LIQUID) + 8 * hasbit(cargo_classes, CC_REFRIGERATED) + 16 * hasbit(cargo_classes, CC_PIECE_GOODS) + 32 * hasbit(cargo_classes, CC_ARMOURED)) {  
1 : return bulk_uncovered_spritegroup_Star2000_graphic;
2 : return good_spriteset_Star2000_graphic;
3 : return bulk_covered_spritegroup_Star2000_graphic;
4 : return oilbox_spritegroup_Star2000_graphic;
8 : return good_spriteset_Star2000_graphic;
16 : return otherbox_spritegroup_Star2000_graphic;
32 : return good_spriteset_Star2000_graphic;
return good_spritegroup_Star2000_graphic;
}
switch(FEAT_ROADVEHS, SELF, box_spriteset_Star2000_graphic, cargo_type_in_veh) {  
BDMT : return bdmtbox_spritegroup_Star2000_graphic;
BRCK : return bdmtbox_spritegroup_Star2000_graphic;
STEL : return stelbox_spritegroup_Star2000_graphic;
WOOD : return woodbox_spritegroup_Star2000_graphic;
GRAI : return bulk_covered_spritegroup_Star2000_graphic;
WHEA : return bulk_covered_spritegroup_Star2000_graphic;
MAIL : return good_spriteset_Star2000_graphic;
VEHI : return good_spriteset_Star2000_graphic;
ENSP : return good_spriteset_Star2000_graphic;
return check_box_cargo_class_spriteset_Star2000_graphic_switch;
}
// check cargo for flat subtype
switch(FEAT_ROADVEHS, SELF, flat_spriteset_Star2000_graphic, cargo_type_in_veh) {  
BDMT : return bdmt_spritegroup_Star2000_graphic;
STEL : return stel_spritegroup_Star2000_graphic;
IRON : return stel_spritegroup_Star2000_graphic;
METL : return stel_spritegroup_Star2000_graphic;
WOOD : return wood_spritegroup_Star2000_graphic;
WDPR : return wdpr_spritegroup_Star2000_graphic;
VEHI : return vehi_spritegroup_Star2000_graphic;
ENSP : return vehi_spritegroup_Star2000_graphic;
GOOD : return goodflat_spritegroup_Star2000_graphic;
return otherflat_spritegroup_Star2000_graphic;
}



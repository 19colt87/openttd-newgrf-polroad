// check cargo for box subtype
switch(FEAT_ROADVEHS, SELF, check_box_cargo_class_spriteset_trailers1n_graphic_switch, hasbit(cargo_classes, CC_BULK) + 2 * hasbit(cargo_classes, CC_COVERED) + 4 * hasbit(cargo_classes, CC_LIQUID) + 8 * hasbit(cargo_classes, CC_REFRIGERATED) + 16 * hasbit(cargo_classes, CC_PIECE_GOODS)) {  
1 : return bulk_uncovered_spritegroup_trailers1n_graphic;
2 : return box_spriteset_trailers1n_graphic;
3 : return bulk_covered_spritegroup_trailers1n_graphic;
4 : return oilbox_spritegroup_trailers1n_graphic;
8 : return box_spriteset_trailers1n_graphic;
16 : return good_spritegroup_trailers1n_graphic;
return good_spritegroup_trailers1n_graphic;
}
switch(FEAT_ROADVEHS, SELF, box_spriteset_trailers1n_graphic_switch, cargo_type_in_veh) {  
BDMT : return brck_spritegroup_trailers1n_graphic;
BRCK : return brck_spritegroup_trailers1n_graphic;
STEL : return stelbox_spritegroup_trailers1n_graphic;
WOOD : return woodbox_spritegroup_trailers1n_graphic;
GRAI : return bulk_covered_spritegroup_trailers1n_graphic;
WHEA : return bulk_covered_spritegroup_trailers1n_graphic;
MAIL : return box_spriteset_trailers1n_graphic;
PAPR : return papr_spritegroup_trailers1n_graphic;
return check_box_cargo_class_spriteset_trailers1n_graphic_switch;
}
// check cargo for flat subtype
switch(FEAT_ROADVEHS, SELF, flat_spriteset_trailers1n_graphic_switch, cargo_type_in_veh) {  
BDMT : return bdmt_spritegroup_trailers1n_graphic;
STEL : return stel_spritegroup_trailers1n_graphic;
METL : return stel_spritegroup_trailers1n_graphic;
IRON : return stel_spritegroup_trailers1n_graphic;
WOOD : return wood_spritegroup_trailers1n_graphic;
WDPR : return wdpr_spritegroup_trailers1n_graphic;
VEHI : return vehi_spritegroup_trailers1n_graphic;
ENSP : return vehi_spritegroup_trailers1n_graphic;
return otherflat_spritegroup_trailers1n_graphic;
}

